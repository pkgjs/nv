{
  "name": "@pkgjs/nv",
  "version": "0.1.0",
  "description": "A tool for resolving node versions from common aliases",
  "author": "Wes Todd <wes@wesleytodd.com>",
  "keywords": [
    "alias",
    "node",
    "node versions",
    "versions",
    "lts",
    "active",
    "maintenence"
  ],
  "license": "MIT",
  "main": "index.js",
  "exports": {
    "require": "./index.js",
    "import": "./.esm-wrapper.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pkgjs/nv.git"
  },
  "scripts": {
    "test": "standard && mocha",
    "debug": "mocha --inspect --inspect-brk --timeout=0",
    "prepublishOnly": "npm t",
    "postpublish": "git push origin && git push origin --tags",
    "prepack": "gen-esm-wrapper . ./.esm-wrapper.mjs"
  },
  "dependencies": {
    "got": "^10.2.1",
    "semver": "^7.1.1"
  },
  "devDependencies": {
    "gen-esm-wrapper": "^1.1.0",
    "mocha": "^6.2.0",
    "standard": "^14.3.1"
  }
}
